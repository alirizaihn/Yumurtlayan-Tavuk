<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alirizaihn.com.yumurtlayantavuk">

    <application
        android:allowBackup="true"
        android:icon="@drawable/chicken"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".GameActivity" android:configChanges="orientation|keyboardHidden" android:screenOrientation="portrait"> </activity>
        <activity android:name=".GameOver" android:configChanges="orientation|keyboardHidden" android:screenOrientation="portrait"> </activity>
    </application>
</manifest>



